#!/bin/bash

if [ -e /var/run/FASTD-<%= @mesh_code %>_FULL ] ; then
    /usr/bin/logger "$0: <%= @mesh_code %> fastd rejects $PEER_KEY because: $(cat /var/run/FASTD-<%= @mesh_code %>_FULL)."
    exit 1
fi

if /bin/grep -Fq $PEER_KEY /etc/fastd/<%= @mesh_code %>-mesh-vpn/fastd-blacklist.json; then
        exit 1
else
        exit 0
fi
