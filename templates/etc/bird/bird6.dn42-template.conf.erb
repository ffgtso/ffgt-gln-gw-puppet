# template for dn42 route exchange via bgp
template bgp dn42 {
  table mesh;
  local as <%= @icvpn_as %>;
  source address <%= @icvpn_ipv6_address %>;
  import where (is_ula() || is_dn42() || is_freifunk());
  export where ((source = RTS_BGP) || (source = RTS_DEVICE)) && (is_ula() || is_freifunk() || is_dn42());
};
